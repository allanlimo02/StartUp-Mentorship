{{#partial "content"}}
    <div class="container p-5 m-5">
        <div class="row m-5">
            <div class="col-md-6 text-center">
                <h1 style="color:rgb(42,74,74);">SME-INCUBATOR</h1>
                <br>
                <br>
                <img src="images/justicon.jpeg" style="width:400px;height:550; border-radius:500px 500px 500px 500px;"/>
            </div>
            <div class="col-md-6">
                <br>
                <br>
                <form action="" method="post" id="loginform">
                    <h1 class="text-center">FILL FORM</h1>
                    <br>
                    <div class="mb-3">
                        <label for="email" class="form-label">Email address</label>
                        <input type="email" name="email" class="form-control" id="email1" aria-describedby="emailHelp" required>
                        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
                    </div>
                    <div class="mb-3">
                        <label for="InputPassword" class="form-label">Password</label>
                        <input type="password" name="InputPassword" class="form-control" id="exampleInputPassword1" required>
                    </div>
                    <div class="mb-3 form-check">
                        <input type="checkbox" name="checkbox" class="form-check-input" id="exampleCheck1">
                        <label class="form-check-label" for="checkbox">Remember</label>
                    </div>
                    <div class="text-center">
                        <button type="submit" class="btn btn-lg btn-primary  text-center" style="background-color:rgb(42,74,74) !important;">Submit</button>
                    </div>

                </form>
            </div>
        </div>


    </div>
{{/partial}}
{{> layout.hbs}}